    "@types/react-dom": "18.3.0",
    "@utrecht/component-library-react": "5.0.0",
    "@nl-design-system-unstable/voorbeeld-design-tokens": "1.0.0-alpha.151",
    "@utrecht/icon": "1.1.0",
    "@vng/assets": "workspace:*",
    "@vng/design-tokens": "workspace:*",
